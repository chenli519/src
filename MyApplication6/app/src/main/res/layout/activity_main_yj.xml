<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@color/titleback"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/current_user"
            android:text="当前未登录"
            android:gravity="center"
            android:layout_gravity="center"
            android:textSize="17sp"
            android:textColor="#ffffffff"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.7"/>
        <TextView

            android:id="@+id/current_position"
            android:text="@string/wdwz"
            android:gravity="center"
            android:layout_gravity="center"
            android:textSize="20sp"
            android:textColor="#ffffffff"
            android:layout_width="0dp"
            android:layout_weight="0.3"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <Spinner
        android:id="@+id/telSpinner"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <AutoCompleteTextView
        android:id="@+id/autoTextview"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        />


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableRow
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:id="@+id/linear01"
                android:layout_weight="1.0"
                >
                <ImageButton
                    android:id="@+id/voice_call"
                    android:text="发起语音呼叫"
                    android:background="@drawable/super_web_27"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    />
                <TextView
                    android:id="@+id/TextViewYYHJ"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="5dp"
                    android:textColor="#ff000000"
                    android:text="@string/title_yyhj"
                    android:textSize="15sp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:id="@+id/linear02"
                android:layout_weight="1.0"
                >
                <ImageButton
                    android:id="@+id/video_call"
                    android:text="发起视频呼叫"
                    android:background="@drawable/super_web_03"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/TextViewSPHJ"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="5dp"
                    android:textColor="#ff000000"
                    android:text="@string/title_sphj"
                    android:textSize="15sp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_weight="1.0"
                >
                <ImageButton
                    android:id="@+id/poc"
                    android:text="发起语音对讲"
                    android:background="@drawable/super_web_40"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/TextViewYYDJ"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="5dp"
                    android:textColor="#ff000000"
                    android:text="@string/title_yjdj"
                    android:textSize="15sp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_weight="1.0"
                >
                <ImageButton
                    android:id="@+id/messages"
                    android:text="消息发送"
                    android:background="@drawable/super_web_26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/TextViewXXFS"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="5dp"
                    android:textColor="#ff000000"
                    android:text="@string/title_xxfs"
                    android:textSize="15sp"
                    />
            </LinearLayout>
        </TableRow>
        <TableRow android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_weight="1.0"
                >
                <ImageButton
                    android:id="@+id/pictureImg"
                    android:text="拍照"
                    android:background="@drawable/super_web_26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/textViewPicture"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="5dp"
                    android:textColor="#ff000000"
                    android:text="拍照"
                    android:textSize="15sp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_weight="1.0"
                >
                <ImageButton
                    android:id="@+id/VideoImg"
                    android:text="视频"
                    android:background="@drawable/super_web_26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/textViewVideo"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginTop="5dp"
                    android:textColor="#ff000000"
                    android:text="视频"
                    android:textSize="15sp"
                    />
            </LinearLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        </TableRow>

        <Button
            android:id="@+id/exit_button"
            android:text="退出"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </TableLayout>

</LinearLayout>
